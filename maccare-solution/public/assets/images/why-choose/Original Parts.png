<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="chipGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF9800;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F57C00;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Circuit board background -->
  <rect x="10" y="10" width="80" height="80" rx="5" fill="#2E7D32" stroke="#1B5E20" stroke-width="2"/>
  
  <!-- Main chip -->
  <rect x="30" y="30" width="40" height="40" rx="3" fill="url(#chipGrad)" stroke="#E65100" stroke-width="1"/>
  
  <!-- Chip pins -->
  <rect x="25" y="35" width="5" height="2" fill="#333"/>
  <rect x="25" y="40" width="5" height="2" fill="#333"/>
  <rect x="25" y="45" width="5" height="2" fill="#333"/>
  <rect x="25" y="50" width="5" height="2" fill="#333"/>
  <rect x="25" y="55" width="5" height="2" fill="#333"/>
  <rect x="25" y="60" width="5" height="2" fill="#333"/>
  
  <rect x="70" y="35" width="5" height="2" fill="#333"/>
  <rect x="70" y="40" width="5" height="2" fill="#333"/>
  <rect x="70" y="45" width="5" height="2" fill="#333"/>
  <rect x="70" y="50" width="5" height="2" fill="#333"/>
  <rect x="70" y="55" width="5" height="2" fill="#333"/>
  <rect x="70" y="60" width="5" height="2" fill="#333"/>
  
  <!-- Circuit traces -->
  <line x1="15" y1="20" x2="85" y2="20" stroke="#FFD700" stroke-width="1"/>
  <line x1="15" y1="25" x2="85" y2="25" stroke="#FFD700" stroke-width="1"/>
  <line x1="15" y1="75" x2="85" y2="75" stroke="#FFD700" stroke-width="1"/>
  <line x1="15" y1="80" x2="85" y2="80" stroke="#FFD700" stroke-width="1"/>
  
  <!-- Apple logo on chip -->
  <g transform="translate(50, 50)">
    <path d="M0 8C0 8 -4 6 -4 3C-4 1 -2 0 0 0C2 0 4 1 4 3C4 6 0 8 0 8Z" fill="white"/>
    <circle cx="2" cy="2" r="1" fill="#FF9800"/>
  </g>
  
  <!-- "ORIGINAL" text -->
  <text x="50" y="95" text-anchor="middle" fill="#4CAF50" font-family="Arial, sans-serif" font-size="8" font-weight="bold">ORIGINAL</text>
</svg>
